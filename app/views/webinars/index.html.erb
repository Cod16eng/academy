<p id="notice"><%= notice %></p>

<h1>Webinars</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Program</th>
      <th>Date from</th>
      <th>Date to</th>
      <th>Category</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @webinars.each do |webinar| %>
      <tr>
        <td><%= webinar.title %></td>
        <td><%= webinar.program %></td>
        <td><%= webinar.date_from %></td>
        <td><%= webinar.date_to %></td>
        <td><%= webinar.category_id %></td>
        <td><%= webinar.user_id %></td>
        <td><%= link_to 'Show', webinar %></td>
        <td><%= link_to 'Edit', edit_webinar_path(webinar) %></td>
        <td><%= link_to 'Destroy', webinar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Webinar', new_webinar_path %>
