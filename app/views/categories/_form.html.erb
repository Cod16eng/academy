<%= form_with(model: category, local: true) do |form| %>


  <div class="field">
    <%= form.label :category_name %>
    <%= form.text_field :category_name %>
  </div>
  <div class="field">
    <%= form.label :category_img %>
    <%= form.file_field :category_img %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
